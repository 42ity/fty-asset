#!/usr/bin/make -f
# -*- makefile -*-

override_dh_strip:
	dh_strip --dbg-package=agent-asset-dbg

override_dh_auto_test:
	echo "Skipped for now"

override_dh_auto_configure:
	dh_auto_configure -- --with-systemd

#MVY: hack to turn off weird error: no dependency information found for /usr/lib/libbiosapi.so.0 I can't fix atm
override_dh_shlibdeps:
	dh_shlibdeps --dpkg-shlibdeps-params=--ignore-missing-info

%:
	dh $@ --with=autoreconf

.PHONY: override_dh_strip override_dh_auto_test override_dh_auto_configure

